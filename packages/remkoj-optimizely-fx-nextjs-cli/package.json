{
  "name": "@remkoj/optimizely-fx-nextjs-cli",
  "type": "module",
  "version": "0.0.2",
  "scripts": {
    "prepare": "rollup -c ./rollup.config.js",
    "watch": "rollup -c ./rollup.config.js -w",
    "compile": "rollup -c ./rollup.config.js",
    "pack:dev": "rollup -c ./rollup.config.js && yarn pack --install-if-needed --out ../../packages/%s-dev.tgz"
  },
  "bin": {
    "opti-fx": "./dist/index.js"
  },
  "files": [
    "./dist"
  ],
  "dependencies": {
    "@next/env": "^16.0.0",
    "handlebars": "^4.7.8",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "yargs": "^18.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-typescript": "^12.3.0",
    "@types/node": "^22.18.12",
    "@types/yargs": "^17.0.34",
    "rollup": "4.52.5",
    "rollup-plugin-string-import": "^1.2.6"
  }
}
